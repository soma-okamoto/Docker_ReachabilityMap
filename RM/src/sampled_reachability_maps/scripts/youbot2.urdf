  </joint>
  
  <link name="arm_link_5">
    <inertial>
      <mass value="0.148"/>
      <origin rpy="0 0 0" xyz="0.000 0.000 0.059"/>
      <inertia ixx="0.000034" ixy="0.000000" ixz="0.000000" iyy="0.000253" iyz="0.000000" izz="0.000249"/>
