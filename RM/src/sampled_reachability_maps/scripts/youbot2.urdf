  <!-- Base Link -->
  <link name="base_link">
    <inertial>
      <mass value="0.0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- Gazebo Specific Settings -->
  <gazebo reference="arm_link_0">
    <material value="youbot/DarkGrey"/>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

</robot>
