    </collision>
  </link>

  <!-- Arm Joint 5 and Link 5 -->
  <joint name="arm_joint_5" type="revolute">
    <origin rpy="0 1.57079632679 0" xyz="0.000 0.000 0.063"/>
    <parent link="arm_link_4"/>
    <child link="arm_link_5"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="1" friction="1"/>
